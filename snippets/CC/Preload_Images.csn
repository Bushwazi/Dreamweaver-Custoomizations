<?xml version="1.0" encoding="utf-8"?><snippet name="Preload Images" description="" preview="code" type="block"><insertText location="beforeSelection"><![CDATA[function preload(arrayOfImages) {    $(arrayOfImages).each(function(){        $('<img/>')[0].src = this;        // Alternatively you could use:        // (new Image()).src = this;    });}// Usage:preload([    'img/imageName.jpg',    'img/anotherOne.jpg',    'img/blahblahblah.jpg']);]]></insertText><insertText location="afterSelection"><![CDATA[]]></insertText></snippet>